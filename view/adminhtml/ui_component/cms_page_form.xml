<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset sortOrder="200" name="history">
        <settings>
            <collapsible>true</collapsible>
            <label translate="true">History</label>
        </settings>
        <htmlContent name="backups" sortOrder="10">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="additionalClasses" xsi:type="array">
                        <item name="admin__field" xsi:type="boolean">true</item>
                    </item>
                </item>
            </argument>
            <block class="Overdose\CMSContent\Block\Adminhtml\Cms\Page\Edit\History"
                   name="overdose.cmscontent.page.history"
                   template="Overdose_CMSContent::cms/backup/history.phtml" />
        </htmlContent>
    </fieldset>
</form>
